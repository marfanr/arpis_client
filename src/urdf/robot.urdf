<?xml version="1.0"?>
<robot name="multipleshapes">
  <link name="myfirst">    
  </link>

<!-- kaki -->
  <link name="kaki1">
    <origin xyz="0.4 0.0 0.0"/>
    <visual>    
      <geometry>
        <box size="1 1 3"/>
      </geometry>              
      <material name="gray">      
        <color rgba="0.37 0.41 0.40 0.8"/>  
      </material>      
    </visual>
  </link>

  <joint name="base_to_kaki1" type="fixed">
    <parent link="myfirst"/>
    <child link="kaki1"/>
    <origin xyz="0.22 0 .25"/>
    <!-- <limit upper="0" lower="-.5" effort="10" velocity="10"/> -->
  </joint>

  <link name="kaki2">
    <origin xyz="1.5 1.0 0.0"/>
    <visual>    
      <geometry>
        <box size="1 1 3"/>
      </geometry>              
      <material name="gray">      
        <color rgba="0.5 0.41 0.20 0.9"/>  
      </material>      
    </visual>
  </link>

  <joint name="base_to_kaki2" type="fixed">
    <parent link="myfirst"/>
    <child link="kaki2"/>
    <origin xyz="2 0 .25"/>
    <!-- <limit upper="0" lower="-.5" effort="10" velocity="10"/> -->
  </joint>  
  

</robot>
